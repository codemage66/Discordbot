    "delay": "^5.0.0",
    "discord.js": "^14.7.1",
    "discord.js-collector-utils": "1.9.1",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-rate-limit": "^6.7.0",
    "highlight.js": "^11.7.0",
    "markdown-it": "^13.0.1",
    "ms": "^2.1.3",
